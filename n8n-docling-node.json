{
  "name": "Docling Document Converter",
  "nodes": [
    {
      "parameters": {
        "url": "https://asista-docling.up.railway.app/convert/url",
        "authentication": "genericCredentialType",
        "genericAuthType": "httpHeaderAuth",
        "sendHeaders": true,
        "headerParameters": {
          "parameters": [
            {
              "name": "Content-Type",
              "value": "application/json"
            }
          ]
        },
        "sendBody": true,
        "bodyParameters": {
          "parameters": [
            {
              "name": "url",
              "value": "={{ $json.documentUrl }}"
            },
            {
              "name": "output_format",
              "value": "={{ $json.outputFormat || 'markdown' }}"
            }
          ]
        },
        "options": {
          "timeout": 120000
        }
      },
      "id": "docling-converter-node",
      "name": "Docling Converter",
      "type": "n8n-nodes-base.httpRequest",
      "typeVersion": 4.2,
      "position": [
        820,
        300
      ],
      "credentials": {}
    }
  ],
  "connections": {},
  "pinData": {}
}
